<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Description</key>
    <string>Downloads the current release version of Latexian creates a package and imports it into Munki.

Latexian has reached EOL (See http://tacosw.com/index.php). Download link might become broken in near future.
Latexian requires MacTeX (See http://www.tug.org/mactex/).
</string>
    <key>Identifier</key>
    <string>com.github.michalmmac.munki.Latexian</string>
    <key>Input</key>
    <dict>
        <key>NAME</key>
        <string>Latexian</string>
        <key>MUNKI_REPO_SUBDIR</key>
        <string>apps/productivity/%NAME%</string>
        <key>pkginfo</key>
        <dict>
            <key>catalogs</key>
            <array>
                <string>testing</string>
            </array>
            <key>description</key>
            <string>Latexian is neat LaTeX editor with live preview.</string>
            <key>category</key>
            <string>Productivity</string>
            <key>developer</key>
            <string>Taco Software</string>
            <key>display_name</key>
            <string>%NAME%</string>
            <key>name</key>
            <string>%NAME%</string>
            <key>minimum_os_version</key>
            <string>10.6.0</string>
            <key>blocking_applications</key>
            <array>
                <string>Latexian</string>
            </array>
            <key>unattended_install</key>
            <true/>
        </dict>
    </dict>
    <key>MinimumVersion</key>
    <string>0.4.0</string>
    <key>ParentRecipe</key>
    <string>com.github.michalmmac.pkg.Latexian</string>
    <key>Process</key>
    <array>
        <dict>
            <key>Processor</key>
            <string>MunkiImporter</string>
            <key>Arguments</key>
            <dict>
                <key>pkg_path</key>
                <string>%pkg_path%</string>
                <key>repo_subdirectory</key>
                <string>%MUNKI_REPO_SUBDIR%</string>
            </dict>
        </dict>
    </array>
</dict>
</plist>
